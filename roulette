# https://ibb.co/CbZfkpS

num = int(input())

# even
even = num % 2
if num == 0:
    print("зеленый")
elif even == 0 and 0 < num < 37:
    if 1 <= num <= 10 or 19 <= num <= 28:
        print("черный")
    elif 11 <= num <= 18 or 29 <= num <= 36:
        print("красный")
elif even != 0 and 0 < num < 37:
    if 1 <= num <= 10 or 19 <= num <= 28:
        print("красный")
    elif 11 <= num <= 18 or 29 <= num <= 36:
        print("черный")

else:
    print("ошибка ввода")
